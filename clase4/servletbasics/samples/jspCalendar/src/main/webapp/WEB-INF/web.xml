<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ParametrosServlet</servlet-name>
        <servlet-class>servlets.ParametrosServlet</servlet-class>
        <init-param>
            <param-name>dia</param-name>
            <param-value>31</param-value>
        </init-param>
        <init-param>
            <param-name>mes</param-name>
            <param-value>12</param-value>
        </init-param>
        <init-param>
            <param-name>annio</param-name>
            <param-value>2019</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>ParametrosServlet</servlet-name>
        <url-pattern>/conparametros</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
